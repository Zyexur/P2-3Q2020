        columnsTitle: "Cedula,Nombre,Apellido,Correo,Teléfono,Fecha de nacimiento,Estado,Rol", ColumnsDataName: "CedulaFisica,Nombre,Apellido,Correo,NumTelefono,FechaNacimiento,EstadoUsuario,Rol",
        onSelectFunction: "BindFields", colorHeader: "white")*@

